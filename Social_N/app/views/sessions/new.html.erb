<div class = "user_new">
<h1>Sign in</h1>
<%= form_tag sessions_path, html: { autocomplete: 'off'} do %>

  <p><%= label_tag :email %></p>
  <%= email_field_tag :email %>

  <p><%= label_tag :password %></p>
  <%= password_field_tag :password %>

  <p><%= submit_tag 'Sign in' %></p>

<% end %>
</div>
